<%- include('header') %>
    <script src="/js/member.js"></script>

    <section style="padding: 20px;">
        <h2 align="center">회원정보수정</h2>
        <form action="/20251223/update" method="post" name="frm" onsubmit="return check()">
            <table border="1" style="margin: 0 auto;">
                <tr>
                    <td>회원번호</td>
                    <td><input type="text" name="custno" value="<%= m.custno %>" readonly></td>
                </tr>
                <tr>
                    <td>회원성명</td>
                    <td><input type="text" name="custname" value="<%= m.custname %>"></td>
                </tr>
                <tr>
                    <td>회원전화</td>
                    <td><input type="text" name="phone" value="<%= m.phone %>"></td>
                </tr>
                <tr>
                    <td>회원주소</td>
                    <td><input type="text" name="address" value="<%= m.address %>"></td>
                </tr>
                <tr>
                    <td>가입일자</td>
                    <td><input type="text" name="joindate" value="<%= m.joindate %>"></td>
                </tr>
                <tr>
                    <td>고객등급[A:VIP, B:일반, C:직원]</td>
                    <td><input type="text" name="grade" value="<%= m.grade %>"></td>
                </tr>
                <tr>
                    <td>거주지역</td>
                    <td><input type="text" name="city" value="<%= m.city %>"></td>
                </tr>
                <tr>
                    <td colspan="2" align="center">
                        <button type="submit">수정</button>
                        <button type="button" onclick="res()">다시하기</button>
                    </td>
                </tr>
            </table>
        </form>
    </section>

    <%- include('footer') %>